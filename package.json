{
  "name": "meu-primeiro-projeto-node",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prebuild": "yarn install",
    "build": "yarn tsc",
    "prestart": "yarn build",
    "start": "ts-node src/shared/infra/http/server.js",
    "predev:server": "yarn typeorm:run",
    "dev:server": "tsnd -r tsconfig-paths/register --inspect --transpile-only --ignore node_modules src/shared/infra/http/server.ts",
    "typeorm": "tsnd -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:create": "yarn typeorm migration:create -n",
    "typeorm:rollback": "yarn typeorm migration:revert",
    "typeorm:show": "yarn typeorm migration:show",
    "typeorm:run": "yarn typeorm migration:run",
    "test": "jest"
  },
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.767.0",
    "bcryptjs": "^2.4.3",
    "celebrate": "^13.0.3",
    "class-transformer": "^0.3.1",
    "cors": "^2.8.5",
    "date-fns": "^2.15.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.6",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.4.6",
    "mongodb": "^3.6.2",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.11",
    "pg": "^8.3.2",
    "rate-limiter-flexible": "^2.1.10",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.3.0",
    "typeorm": "^0.2.25",
    "uuidv4": "^6.2.3"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/handlebars": "^4.1.0",
    "@types/ioredis": "^4.17.4",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mime": "^2.0.3",
    "@types/mongodb": "^3.5.27",
    "@types/multer": "^1.4.4",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.27",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "prettier": "^2.1.0",
    "ts-jest": "^26.3.0",
    "ts-node-dev": "^1.0.0-pre.60",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2"
  }
}
